{
  "name": "dermaclinicsnearme",
  "version": "1.0.0",
  "description": "A comprehensive directory of dermatology clinics across the USA",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "collect-data": "tsx scripts/collectClinicData.ts",
    "fetch:unsplash": "node scripts/fetch-unsplash-images.js",
    "pages:build": "npx @opennextjs/cloudflare@latest build && cp .open-next/worker.js .open-next/_worker.js",
    "preview": "npm run pages:build && wrangler pages dev .open-next",
    "deploy": "npm run pages:build && wrangler pages deploy .open-next"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.11.0",
    "@supabase/supabase-js": "^2.75.1",
    "@types/google.maps": "^3.55.0",
    "dotenv": "^17.2.3",
    "next": "^15.5.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
